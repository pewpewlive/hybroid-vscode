{
  "name": "hybroid",
  "displayName": "Hybroid",
  "description": "Hybroid language support for Visual Studio Code",
  "publisher": "HybroidTeam",
  "repository": {
    "url": "https://github.com/pewpewlive/hybroid-vscode"
  },
  "bugs": {
    "url": "https://github.com/pewpewlive/hybroid-vscode/issues"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "icon": "assets/icon.png",
  "contributes": {
    "languages": [
      {
        "id": "hybroid",
        "aliases": [
          "Hybroid",
          "hybroid"
        ],
        "extensions": [
          ".hyb",
          ".hybroid"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "hybroid",
        "scopeName": "source.hybroid",
        "path": "./syntaxes/hybroid.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "hybroid",
        "path": "./snippets/hybroid.code-snippets"
      }
    ]
  }
}
